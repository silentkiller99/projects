<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>UserInformation</title>
</head>
<body>
	<table>
		<thead>
			<tr>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Date of Birth</th>
				<th>File</th>
			</tr>
		</thead>
		<tbody>
			<% results.forEach((user) => { %>
				<tr>
					<td><%= user.firstName %></td>
					<td><%= user.lastName %></td>
					<td><%= user.dateOfBirth %></td>
					<td><a href="/download/<%= user.imagePath %>" download>Download</a>
						<a href="/view/<%= user.imagePath %>" >View</a>
					</td>
				</tr>
			<% }); %>
		</tbody>
	</table>
  <a href="/download/data.csv" download>Download CSV</a>

</body>
</html>
